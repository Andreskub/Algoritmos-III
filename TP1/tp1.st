Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'tp1-105238'!!AlgoVid methodsFor: 'initialization' stamp: 'Andres 5/15/2021 20:33'!initialize 	personas := OrderedCollection new.	burbujas := OrderedCollection new.	colegios := OrderedCollection new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 17:45'!burbujaPinchada: unNombreBurbuja 	^(self buscarBurbuja: unNombreBurbuja) burbujaPinchada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:22'!puedeCircular: unNombrePersona 	^(self buscarPersona: unNombrePersona) puedeCircular.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:49'!diagnosticoDe: unNombrePersona 	^(self buscarPersona: unNombrePersona ) diagnosticoPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:28'!contactoEstrechoEntre: unNombrePersona y: otroNombrePersona	| personaUno personaDos |	personaUno := (self buscarPersona: unNombrePersona).	personaDos := (self buscarPersona: otroNombrePersona).		personaUno agregarContactoEstrecho: personaDos.	personaDos agregarContactoEstrecho: personaUno.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:45'!agregarA: unNombreBurbuja alColegio: unNombreColegio 	 (self buscarColegio: unNombreColegio) agregarBurbuja: (self buscarBurbuja: unNombreBurbuja).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:10'!nuevaPersona: unNombrePersona	| unaPersona |	unaPersona := Persona new.	unaPersona nombre: unNombrePersona.	personas add: unaPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:19'!buscarPersona: unNombrePersona     | personaBuscada |    personaBuscada := personas detect: [ :persona | (persona nombre) = unNombrePersona ].    ^personaBuscada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:37'!agregarSintoma: unNombreSintoma a: unNombrePersona 	(self buscarPersona: unNombrePersona) agregarSintoma: unNombreSintoma.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:31'!agregarA: unNombrePersona aLaBurbuja: unNombreBurbuja 	(self buscarBurbuja: unNombreBurbuja ) nuevaPersonaEnBurbuja: (self buscarPersona: unNombrePersona). ! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:56'!clasesPresencialesEn: unNombreColegio	^(self buscarColegio: unNombreColegio) colegioEsPresencial.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:41'!esPersonaDeRiesgo: unNombrePersona 	(self buscarPersona: unNombrePersona) esGrupoDeRiesgo. ! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:07'!esPersonalEsencial: unNombrePersona 	(self buscarPersona: unNombrePersona) esEsencial.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:38'!nuevoColegio: unNombreColegio	| unColegio |	unColegio := Colegio new.	unColegio nombreColegio: unNombreColegio. 	colegios add: unColegio.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 22:23'!estaVacunadx: unNombrePersona 	^((self buscarPersona: unNombrePersona) vacunar)estaVacunadx.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:45'!buscarColegio: unNombreColegio	| colegioBuscado |    colegioBuscado := colegios detect: [ :colegio | (colegio nombreColegio) = unNombreColegio ].    ^colegioBuscado.	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 00:13'!nuevaBurbuja: unNombreBurbuja	| unaBurbuja |	unaBurbuja := Burbuja new.	unaBurbuja nombre: unNombreBurbuja.	burbujas add: unaBurbuja.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 00:22'!buscarBurbuja: unNombreBurbuja 	| burbujaBuscada |    burbujaBuscada := burbujas detect: [ :burbuja | (burbuja nombre) = unNombreBurbuja ].    ^burbujaBuscada.! !Object subclass: #Burbuja	instanceVariableNames: 'personasBurbuja nombre pinchada saludBurbuja'	classVariableNames: ''	package: 'tp1-105238'!!Burbuja methodsFor: 'initialization' stamp: 'Andres 5/15/2021 16:13'!initialize 	personasBurbuja := OrderedCollection new.	saludBurbuja := BurbujaNoEstaPinchada new.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:30'!nuevaPersonaEnBurbuja: unaPersona	personasBurbuja add: unaPersona.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:28'!burbujaPinchada 	personasBurbuja detect: [:persona | (persona diagnosticoPersona) = 'Positivo'] ifFound: [saludBurbuja := BurbujaSiEstaPinchada new].	saludBurbuja diagnosticarPersonasBurbuja: personasBurbuja .	^saludBurbuja estaPinchada.! !!Burbuja methodsFor: 'accessing' stamp: 'Andres 5/15/2021 00:13'!nombre: unNombreBurbuja 	nombre := unNombreBurbuja.! !!Burbuja methodsFor: 'accessing' stamp: 'Andres 5/15/2021 00:11'!nombre	^nombre.! !Object subclass: #BurbujaNoEstaPinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!BurbujaNoEstaPinchada methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 17:37'!diagnosticarPersonasBurbuja: personasBurbuja 	nil.! !!BurbujaNoEstaPinchada methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:26'!estaPinchada	^true.! !Object subclass: #BurbujaSiEstaPinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!BurbujaSiEstaPinchada methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 17:25'!diagnosticarPersonasBurbuja: personasBurbuja 	personasBurbuja do: [:persona | persona noPuedeCircular].! !!BurbujaSiEstaPinchada methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 15:59'!estaPinchada 	^true.! !Object subclass: #ClasesNoPresenciales	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!ClasesNoPresenciales methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:50'!esPresencial	^false.! !Object subclass: #ClasesSiPresenciales	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!ClasesSiPresenciales methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:50'!esPresencial 	^true.! !Object subclass: #Colegio	instanceVariableNames: 'burbujasColegio burbujas nombreColegio presencialidad'	classVariableNames: ''	package: 'tp1-105238'!!Colegio methodsFor: 'accessing' stamp: 'Andres 5/15/2021 20:36'!nombreColegio	^nombreColegio.! !!Colegio methodsFor: 'accessing' stamp: 'Andres 5/15/2021 20:36'!nombreColegio: unNombreColegio	nombreColegio := unNombreColegio.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 22:52'!colegioEsPresencial	| cantidadBurbujasPinchadas |	cantidadBurbujasPinchadas := 0.	burbujas do: [ :burbuja | (burbuja burbujaPinchada) ifTrue: [ cantidadBurbujasPinchadas := cantidadBurbujasPinchadas+1 ] ].	(cantidadBurbujasPinchadas > (0.4 * burbujas size)) ifTrue: [ presencialidad := ClasesNoPresenciales new ].	^presencialidad esPresencial.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:44'!agregarBurbuja: unaburbuja	burbujas add: unaburbuja. ! !!Colegio methodsFor: 'initialization' stamp: 'Andres 5/15/2021 20:51'!initialize 	burbujas := OrderedCollection new.	presencialidad := ClasesSiPresenciales new.! !Object subclass: #Persona	instanceVariableNames: 'nombre personaNoCircula circula vacunado sintomas salud escencial esencial contactoEstracho contactoEstrecho esRiesgo esGrupoDeRiesgo grupoDeRiesgo'	classVariableNames: ''	package: 'tp1-105238'!!Persona methodsFor: 'accessing' stamp: 'Andres 5/11/2021 18:20'!nombre 	^nombre.! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/15/2021 00:12'!nombre: unNombrePersona 	nombre := unNombrePersona .! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/13/2021 23:42'!esGrupoDeRiesgo	grupoDeRiesgo := PersonaSiEsDeRiesgo new.	circula := PersonaNoCircula new.! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/11/2021 22:19'!vacunar	vacunado := PersonaSiEstaVacunada new.	circula := PersonaSiCircula new.! !!Persona methodsFor: 'initialization' stamp: 'Andres 5/13/2021 23:38'!initialize 	circula := PersonaNoCircula new.	vacunado := PersonaNoEstaVacunada new.	salud := SaludPersona new.	esencial := PersonaNoEscencial new.	contactoEstrecho := OrderedCollection new.	grupoDeRiesgo := PersonaNoEsDeRiesgo new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 21:00'!estaVacunadx	^vacunado estaVacunadx.	! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:53'!agregarSintoma: unNombreSintoma 	salud nuevaSintoma: unNombreSintoma.	circula := PersonaNoCircula new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:32'!agregarContactoEstrecho: unaPersona 	contactoEstrecho add: unaPersona.	(unaPersona diagnosticoPersona = 'Sospechoso') ifTrue: (circula := PersonaNoCircula new).	! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:13'!noPuedeCircular	circula := PersonaNoCircula new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:48'!diagnosticoPersona	^salud diagnosticoPersona.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:19'!esEsencial	esencial := PersonaSiEscencial new.	grupoDeRiesgo actualizarCirculacion: self.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:16'!siPuedeCircular	circula := PersonaSiCircula new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:34'!puedeCircular 	^circula puedeCircular.! !Object subclass: #PersonaNoCircula	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaNoCircula methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:28'!puedeCircular 	^false.! !Object subclass: #PersonaNoEsDeRiesgo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaNoEsDeRiesgo methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:20'!actualizarCirculacion: unaPersona 	unaPersona siPuedeCircular.! !!PersonaNoEsDeRiesgo methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:42'!esRiesgo	^false.! !Object subclass: #PersonaNoEscencial	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaNoEscencial methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:09'!esEsencial 	^false.! !Object subclass: #PersonaNoEstaVacunada	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaNoEstaVacunada methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 21:01'!estaVacunadx 	^false.! !Object subclass: #PersonaSiCircula	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaSiCircula methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 22:19'!puedeCircular 	^true.! !Object subclass: #PersonaSiEsDeRiesgo	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaSiEsDeRiesgo methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:14'!actualizarCirculacion: unaPersona 	unaPersona noPuedeCircular.	! !!PersonaSiEsDeRiesgo methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:43'!esRiesgo	^true.! !Object subclass: #PersonaSiEscencial	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaSiEscencial methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:09'!esEsencial 	^true.! !Object subclass: #PersonaSiEstaVacunada	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaSiEstaVacunada methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 21:01'!estaVacunadx 	^true.! !Object subclass: #SaludPersona	instanceVariableNames: 'sintomas diagnostico'	classVariableNames: ''	package: 'tp1-105238'!!SaludPersona methodsFor: 'initialization' stamp: 'Andres 5/13/2021 22:38'!initialize 	sintomas := OrderedCollection new.	diagnostico := 'Saludable'.! !!SaludPersona methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:34'!diagnosticoPersona	(sintomas size ~= 0) ifTrue: [ diagnostico := 'Sospechoso' ].	(sintomas size = 3) ifTrue: [         sintomas detect: [ :x| (x sintomaHabitual)] ifFound: [diagnostico := 'Positivo']].   ((sintomas size >= 4)) ifTrue: [diagnostico := 'Positivo'].	^diagnostico.! !!SaludPersona methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 20:56'!nuevaSintoma: unNuevoSintoma	| unSintoma |	unSintoma := Sintoma new.	unSintoma nombreSintoma: unNuevoSintoma .	sintomas add: unSintoma .! !Object subclass: #Sintoma	instanceVariableNames: 'nombreSintoma sintomasHabituales'	classVariableNames: ''	package: 'tp1-105238'!!Sintoma methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:59'!sintomaHabitual	^ sintomasHabituales includes: (self nombreSintoma). ! !!Sintoma methodsFor: 'accessing' stamp: 'Andres 5/13/2021 20:50'!nombreSintoma	^nombreSintoma.! !!Sintoma methodsFor: 'accessing' stamp: 'Andres 5/13/2021 20:49'!nombreSintoma: unNombreSintoma	nombreSintoma := unNombreSintoma. ! !!Sintoma methodsFor: 'initialization' stamp: 'Andres 5/14/2021 00:35'!initialize 	sintomasHabituales := OrderedCollection new.	sintomasHabituales add: 'Fiebre'; add: 'Tos'; add: 'Cansancio'.! !