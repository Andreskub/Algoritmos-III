Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas'	classVariableNames: ''	package: 'tp1-105238'!!AlgoVid methodsFor: 'initialization' stamp: 'Andres 5/15/2021 00:04'!initialize 	personas := OrderedCollection new.	burbujas := OrderedCollection new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:49'!diagnosticoDe: unNombrePersona 	^(self buscarPersona: unNombrePersona ) diagnosticoPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:22'!puedeCircular: unNombrePersona 	^(self buscarPersona: unNombrePersona) puedeCircular.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:28'!contactoEstrechoEntre: unNombrePersona y: otroNombrePersona	| personaUno personaDos |	personaUno := (self buscarPersona: unNombrePersona).	personaDos := (self buscarPersona: otroNombrePersona).		personaUno agregarContactoEstrecho: personaDos.	personaDos agregarContactoEstrecho: personaUno.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:10'!nuevaPersona: unNombrePersona	| unaPersona |	unaPersona := Persona new.	unaPersona nombre: unNombrePersona.	personas add: unaPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:19'!buscarPersona: unNombrePersona     | personaBuscada |    personaBuscada := personas detect: [ :persona | (persona nombre) = unNombrePersona ].    ^personaBuscada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:37'!agregarSintoma: unNombreSintoma a: unNombrePersona 	(self buscarPersona: unNombrePersona) agregarSintoma: unNombreSintoma.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 00:35'!agregarA: unNombrePersona aLaBurbuja: unNombreBurbuja 	(self buscarBurbuja: unNombreBurbuja ) nuevaPersonaEnBurbuja: unNombrePersona. ! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:41'!esPersonaDeRiesgo: unNombrePersona 	(self 