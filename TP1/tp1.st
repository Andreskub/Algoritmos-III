ize = 3) ifTrue: [         sintomas detect: [ :x| (x sintomaHabitual)] ifFound: [ salud actualizarDiagnosticoPositivo ]].   ((sintomas size >= 4)) ifTrue: [ salud actualizarDiagnosticoPositivo].! !!DiagnosticoPersonaSospechosa methodsFor: 'as yet unclassified' stamp: 'Andres 5/18/2021 22:13'!diagnostico	^'Sospechoso'.! !Object subclass: #Esencial	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!Object subclass: #Persona	instanceVariableNames: 'nombre personaNoCircula circula vacunado sintomas salud escencial esencial contactoEstrecho esRiesgo esGrupoDeRiesgo grupoDeRiesgo'	classVariableNames: ''	package: 'tp1-105238'!!Persona methodsFor: 'initialization' stamp: 'Andres 5/19/2021 23:59'!initialize 	circula := PersonaNoCircula new.	vacunado := PersonaNoEstaVacunada new.	salud := SaludPersona new.	esencial := PersonaNoEscencial new.	contactoEstrecho := OrderedCollection new.	grupoDeRiesgo := PersonaNoEsDeRiesgo new.! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/15/2021 00:12'!nombre: unNombrePersona 	nombre := unNombrePersona .! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/11/2021 18:20'!nombre 	^nombre.! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/13/2021 23:42'!esGrupoDeRiesgo	grupoDeRiesgo := PersonaSiEsDeRiesgo new.	circula := PersonaNoCircula new.! !!Persona methodsFor: 'accessing' stamp: 'Andres 5/18/2021 14:07'!vacunar	vacunado := PersonaSiEstaVacunada new.	grupoDeRiesgo actualizarCirculacion: self.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 21:00'!estaVacunadx	^vacunado estaVacunadx.	! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/20/2021 00:21'!agregarSintoma: unNombreSintoma 	salud nuevoSintoma: unNombreSintoma.	circula := PersonaNoCircula new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/18/2021 19:00'!agregarContactoEstrecho: unaPersona 	contactoEstrecho add: unaPersona.	(unaPersona diagnosticoPersona = 'Sospechoso') ifTrue: (circula := PersonaNoCircula new).	! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:13'!noPuedeCircular	circula := PersonaNoCircula new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 22:48'!diagnosticoPersona	^salud diagnosticoPersona.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:19'!esEsencial	esencial := PersonaSiEscencial new.	grupoDeRiesgo actualizarCirculacion: self.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/14/2021 00:16'!siPuedeCircular	circula := PersonaSiCircula new.! !!Persona methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:34'!puedeCircular 	^circula puedeCircular.! !Circula subclass: #PersonaNoCircula	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaNoCircula methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 18:28'!puedeCircular 	^false.! !Esencial subclass: #PersonaNoEscencial	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaNoEscencial methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:09'!esEsencial 	^false.! !Circula subclass: #PersonaSiCircula	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaSiCircula methodsFor: 'as yet unclassified' stamp: 'Andres 5/11/2021 22:19'!puedeCircular 	^true.! !Esencial subclass: #PersonaSiEscencial	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!PersonaSiEscencial methodsFor: 'as yet unclassified' stamp: 'Andres 5/13/2021 23:09'!esEsencial 	^true.! !Object subclass: #Prescencialidad	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!Prescencialidad subclass: #ClasesNoPresenciales	instanceVariableNames: ''	classVariableNames: ''	package: 'tp1-105238'!!ClasesNoPresenciales methodsFor: 'as yet unclassified' stamp: 'Andres 5/15/2021 20:50'!esPresencial